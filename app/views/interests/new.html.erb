<h1>New Interest</h1>


<%= form_tag(interests_url, method: 'post') do %>


  <div>
    <%= label_tag :teacher_id %><br />
    <%= select_tag :teacher_id, options_from_collection_for_select(Teacher.all, :id, :full_name) %>
  </div>

  <div>
    <%= label_tag :course_id %><br />
    <%= select_tag :course_id, options_from_collection_for_select(Course.all, :id, :name) %>
  </div>


  <div>
    <%= submit_tag "Create Interest" %>
  </div>
<% end %>
