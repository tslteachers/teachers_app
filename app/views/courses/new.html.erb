<h1>New Course</h1>


<%= form_tag(courses_url, method: 'post') do %>


  <div>
    <%= label_tag :subject_id %><br />
    <%= number_field_tag :subject_id %>
  </div>

  <div>
    <%= label_tag :location %><br />
    <%= text_field_tag :location %>
  </div>

  <div>
    <%= label_tag :time %><br />
    <%= text_field_tag :time %>
  </div>

  <div>
    <%= label_tag :compensation %><br />
    <%= text_field_tag :compensation %>
  </div>

  <div>
    <%= label_tag :qualifications %><br />
    <%= text_field_tag :qualifications %>
  </div>

  <div>
    <%= label_tag :college_id %><br />
    <%= number_field_tag :college_id %>
  </div>

  <div>
    <%= label_tag :description %><br />
    <%= text_area_tag :description %>
  </div>


  <div>
    <%= submit_tag "Create Course" %>
  </div>
<% end %>
